{
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://widget.uservoice.com/; object-src 'self'",
   "description": "Replace new tab screen with current time and weather.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "96": "images/icon96.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "currently show",
   "options_page": "index.html#options",
   "permissions": [ "storage", "geolocation", "unlimitedStorage", "https://maps.googleapis.com/maps/api/", "http://api.wunderground.com/api", "https://ssl.google-analytics.com/", "https://s3.amazonaws.com/" ],
   "update_url": "http://localhost/updates.xml",
   "version": "2.8.0",
   "web_accessible_resources": [ "index.html", "images/icon128.png" ]
}
